  const ele = document.createElement('link')
  ele.rel = 'stylesheet'
  ele.href= 'https://cdn.jsdelivr.net/npm/gitalk/dist/gitalk.min.css'
  document.getElementsByTagName('head')[0].appendChild(ele)
}

function loadGitalk () {
  function initGitalk () {
    var gitalk = new Gitalk({
      clientID: '658353c677f184d58eb6',
      clientSecret: '59a71d3cc97edd21a138aec8aaeb0887110cb28a',
      repo: 'akilarlxh.github.io',
      owner: 'Akilarlxh',
      admin: ['Akilarlxh'],
