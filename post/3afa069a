  //reset readmode
  $('body').hasClass('read-mode') && $('body').removeClass('read-mode')

})</script></div><div class="GalMenu GalDropDown"><div class="circle" id="gal"><div class="ring"><a href="/" class="menuItem">首页</a> <a href="/archives/" class="menuItem">时间轴</a> <a href="/tags/" class="menuItem">标签</a> <a href="/random/" class="menuItem">随便看看</a> <a href="/comments/" class="menuItem">友人帐</a> <a href="/comments/" class="menuItem">留言板</a></div></div></div><script type="text/javascript">for(var items=document.querySelectorAll(".menuItem"),i=0,l=items.length;i<l;i++)items[i].style.left=(50-35*Math.cos(-.5*Math.PI-1/l*2*i*Math.PI)).toFixed(4)+"%",items[i].style.top=(50+35*Math.sin(-.5*Math.PI-1/l*2*i*Math.PI)).toFixed(4)+"%"</script><script>window.jQuery || document.write('<script src="https://cdn.jsdelivr.net/gh/zyoushuo/Blog@master/hexo/js/jquery-2.1.1.min.js"></script>')<script src="/js/GalMenu.js"></script><script type="text/javascript">$(document).ready(function(){$("body").GalMenu({menu:"GalDropDown"})})</script></body></html>